{
  "license": "MIT",
  "version": "1.0.5",
  "name": "react-turkey-map",
  "main": "./build/TurkeyMap.js",
  "description": "Customizable Turkey map",
  "author": {
    "name": "Ozgur Ozer",
    "email": "ozgr@live.com",
    "url": "https://github.com/ozgrozer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozgrozer/react-turkey-map.git"
  },
  "scripts": {
    "dev": "parcel ./example/index.html --dist-dir ./dist --public-url /",
    "build": "npm run build1 && npm run build2 && npm run build3 && npm run build4",
    "build1": "rm -rf ./.parcel-cache && rm -rf ./build",
    "build2": "parcel build ./src/TurkeyMap.jsx --dist-dir build --public-url ./ --no-source-maps",
    "build3": "terser ./build/TurkeyMap.js -o ./build/TurkeyMap.min.js",
    "build4": "rm ./build/TurkeyMap.js && mv ./build/TurkeyMap.min.js ./build/TurkeyMap.js"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "sass": "^1.77.4",
    "standard": "^17.1.0",
    "terser": "^5.31.0"
  },
  "keywords": [
    "map",
    "react",
    "harita",
    "turkey",
    "turkiye",
    "turkey-map",
    "turkiye-haritasi",
    "react-turkey-map",
    "react-turkiye-haritasi"
  ]
}
